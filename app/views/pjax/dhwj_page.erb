<div class="main clear">
    <div class="box">
        <div class="m_title">
            <h2>当前位置：服务/大会文件</h2>
        </div>
        <div class="m_body">
            <div class="article_list">
                <ul>
                    <% @list.each do |hsh| %>
                    <li><a href="/dahuiwenjian/<%= URI.escape(hsh[:filename]) %>"><%~ hsh[:filename] %></a><span><%~ hsh[:atime] %></span></li>
                    <% end %>
                </ul>
            </div>

            <%= paginate CustomPagination.new(@count,@page,@per_page) %>
        </div>
    </div>
</div>