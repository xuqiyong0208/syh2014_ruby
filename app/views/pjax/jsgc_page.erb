<div class="main clear">
	<div class="box">
    	<div class="m_title">
        	<h2>当前位置：赛事/竞赛规程</h2>
        </div>
        <div class="m_body">
        	<div class="competition">
                <div class="group_btn">
                    <a href="javascript:;" class="hover"><span>青少年组</span></a>
                    <a href="javascript:alert('暂时还没有高校组的数据');"><span>高校组</span></a>
                </div>
                <div class="competition_con">
                	<div class="rule_list">
                    	<ul>
                            <% @projects.each do |project| %>
                            <% xmmc = project[:xmmc].to_s.strip %>
                            <li><a target="_blank" href="/files/jsgc/<%~ URI.escape(xmmc) %>.pdf"><img src="<%= static_url "/images/rule_#{project[:dhkey]}.png" %>" alt="" /><p><%~ xmmc %></p></a></li>
                            <% end %>
                            <li><a target="_blank" href="/files/jsgc/<%~ URI.escape("单项竞赛规程补充通知") %>.pdf"><img src="<%= static_url "/images/rule_buchongtongzhi.png" %>" alt="" /><p>补充通知</p></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>