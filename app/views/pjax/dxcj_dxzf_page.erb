<div class="main clear">
	<div class="box">
    	<div class="m_title">
        	<h2>当前位置：赛事/单项成绩/<span style="cursor:pointer;" onclick="window.location.href='/dxcj/dxzf';">单项总分</span></h2>
        </div>
        <div class="m_body">
        	  <div class="competition">
            	
                <div class="group_btn">
                  <a href="javascript:;" class="hover"><span>青少年组</span></a>
                  <a href="javascript:alert('暂时还没有高校组的数据');"><span>高校组</span></a>
                </div>

                <div class="competition_a">
                  <a href="/dxcj" class="c_btn_a"><i></i>单项成绩</a>
                  <a href="/dxcj/dxzf" class="c_btn_a hover"><i></i>单项总分</a>
                  <a href="/dxcj/dxjp" class="c_btn_a"><i></i>单项奖牌</a>
                </div>

                <div class="competition_con">
                  <div class="rule_list">
                      <ul>
                            <% @projects.each do |project| %>
                            <% xmmc = project[:xmmc].to_s.strip %>
                            <li><a href="/dxcj/dxzf/<%~ project[:dhkey] %>"><img src="<%= static_url "/images/rule_#{project[:dhkey]}.png" %>" alt="" /><p><%~ xmmc %></p></a></li>
                            <% end %>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>