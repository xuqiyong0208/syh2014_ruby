<div class="main clear">
	<div class="box">
    	<div class="m_title">
        	<h2>当前位置：赛事/排行榜/<span style="cursor:pointer;" onclick="window.location.href='/phb/dxzf';">单项奖牌</span>/<span style="cursor:pointer;" onclick="window.location.href='/phb/dxzf/<%~ @project[:dhkey] %>';"><%~ @project[:xmmc].to_s.strip %></span>/<span style="cursor:pointer;" onclick="window.location.href='/phb/dxzf/<%~ @project[:dhkey] %>?zubie=<%~ URI.escape(@zubie) %>';"><%~ @zubie %></h2>
        </div>
        <div class="m_body">
        	  <div class="competition">
            	
                <div class="group_btn">
                  <a href="javascript:;" class="hover"><span>青少年组</span></a>
                  <a href="javascript:alert('暂时还没有高校组的数据');"><span>高校组</span></a>
                </div>

                <div class="competition_a">
                  <a href="/phb" class="ca01">比赛奖牌</a>
                  <a href="/phb/bszf" class="ca02">比赛总分</a>
                  <a href="/phb/dxjp" class="ca01">单项奖牌</a>
                  <a href="/phb/dxzf" class="ca02 hover">单项总分</a>
                  <a href="/phb/zdxmjp" class=""><i></i>重点项目奖牌</a>
                  <a href="/phb/zdxmzf"><i></i>重点项目总分</a>
                </div>

                <div class="competition_con">
                    <div>
                      <div class="cc_table_left">
                      <table width="200" border="0" cellspacing="5" cellpadding="0">
                          <thead>
                          <tr>
                            <th>县区</th>
                            <th>总分</th>
                          </tr>
                          </thead>
                          <tbody>
                          <% @score.each do |score| %>
                          <tr>
                            <td><%~ score[:dwjc].to_s.strip %></td>
                            <td><%~ score[:mcsum].to_i %></td>
                          </tr>
                          <% end %>
                          </tbody>
                        </table>
                        </div>
                        <div class="" style="width: 905px;float: left;overflow-x: auto;">
                            <table width="100%" border="0" cellspacing="5" cellpadding="0">
                              <thead>
                              <tr>
                                <th>第一名</th>
                                <th>第二名</th>
                                <th>第三名</th>
                                <th>第四名</th>
                                <th>第五名</th>
                                <th>第六名</th>
                                <th>第七名</th>
                                <th>第八名</th>
                                <th>第9-16名</th>
                              </tr>
                              </thead>
                              <tbody>
                              <% @score.each do |score| %>
                              <tr>
                                <td><%~ (tmp=score[:mc1].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc2].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc3].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc4].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc5].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc6].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc7].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc8].to_i)>0 ? tmp : '' %></td>
                                <td><%~ (tmp=score[:mc9].to_i)>0 ? tmp : '' %></td>
                              </tr>
                              <% end %>
                              </tbody>
                            </table>
                        </div>
                        
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>