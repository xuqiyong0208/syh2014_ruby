<div class="main clear">
	<div class="box">
    	<div class="m_title">
        	<h2>当前位置：赛事/排行榜/<span style="cursor:pointer;" onclick="window.location.href='/phb/bszf';">比赛总分</span></h2>
        </div>
        <div class="m_body">
        	  <div class="competition">
            	
                <div class="group_btn">
                  <a href="/phb/bsjp"><span>青少年组</span></a>
                  <a href="javascript:;" class="hover"><span>高校组</span></a>
                </div>

                <div class="competition_a">
                  <a href="/phb/gxbsjp" class="c_btn_a"><i></i>比赛奖牌</a>
                  <a href="/phb/gxbszf" class="c_btn_a hover"><i></i>比赛总分</a>
                </div>

                <div class="competition_con">
                    <div>
                      <div class="cc_table_left">
                        <table width="280" border="0" cellspacing="5" cellpadding="0">
                            <thead>
                              <tr>
                                <th>单位</th>
                                <th>总分</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% $gxzf_score.each do |jiangpai| %>
                              <tr>
                                <td><%~ jiangpai[0] %></td>
                                <td><%~ jiangpai[1] %></td>
                              </tr>
                              <% end %>
                            </tbody>
                          </table>
                        </div>

                        <div class="cc_table_right" style="width:643px;">
                          <table width="100%" border="0" cellspacing="5" cellpadding="0">
                            <thead>
                              <tr>
                                <% $gxzf_projects.each do |xmmc| %>
                                <th><%~ xmmc %></th>
                                <% end %>
                              </tr>
                            </thead>
                            <tbody>
                              <% $gxzf_score.each do |jiangpai| %>
                              <tr>
                                <% $gxzf_projects.each_with_index do |xmmc,i| %>
                                <td><%~ jiangpai[2+i*1] %></td>
                                <% end %>
                              </tr>
                              <% end %>
                            </tbody>
                          </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>