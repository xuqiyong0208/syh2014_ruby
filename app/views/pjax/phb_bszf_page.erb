<script type="text/javascript">

  function load_phb_bszf_sublist_json() {
    $.ajax({
        url:'/phb/bszf/sublist_json',
        type:"get",
        dataType:"json",
        data:{},
        success:function(json){
          for(var key in json){
            // alert(key);  
            // alert(json[key]);  
            // alert("zdf:"+json[key].zdf);
            if(json[key].zdf){
              $('#zf_'+key).html(''+json[key].zdf);
            }
          }
        },
        error:function(info){}
    });
  }

  load_phb_bszf_sublist_json();

</script>