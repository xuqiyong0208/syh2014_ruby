<div class="main clear">
	<div class="box">
    	<div class="m_title">
        	<h2>当前位置：赛事/排行榜</h2>
        </div>
        <div class="m_body">
        	<div class="competition">
            	
                <div class="competition_con">
                	<table width="100%" border="0" cellspacing="5" cellpadding="0">
                      <thead>
                      <tr>
                        <th>序号</th>
                        <th>县区</th>
                        <th class="medal_box">金<i class="one">金牌</i></th>
                        <th class="medal_box">银<i class="two">银牌</i></th>
                        <th class="medal_box">铜<i class="three">铜牌</i></th>
                        <th>总分</th>
                      </tr>
                      </thead>
                      <tbody>
                      <% @jiangpais.each_with_index do |jiangpai, i| %>
                      <tr>
                        <td><%= i+1 %>.</td>
                        <td><%~ jiangpai[:dwjc].to_s.strip %></td>
                        <td><%~ jiangpai[:zjp].to_i %></td>
                        <td><%~ jiangpai[:zyp].to_i %></td>
                        <td><%~ jiangpai[:ztp].to_i %></td>
                        <td><%~ jiangpai[:zdf] %></td>
                      </tr>
                      <% end %>
                      </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>